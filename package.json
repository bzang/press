{
  "name": "@ianwremmel/press",
  "version": "0.0.0-development",
  "description": "Progressive enhancement with VueJS",
  "bin": {},
  "main": "./src/index.js",
  "jsdelivr": "./press.js",
  "style": "./src/style.css",
  "repository": {
    "type": "git",
    "url": "git://github.com/ianwremmel/press.git"
  },
  "scripts": {
    "prebuild": "rimraf dist press.js press.js.map",
    "build": "npm-run-all build:*",
    "build:cdn": "BUILD_TARGET=cdn webpack",
    "build:readme": "markdown-toc -i --bullets='-' --maxdepth=3  README.md  && prettier --write README.md",
    "commitmsg": "commitlint -e",
    "lint": "npm-run-all lint:*",
    "eslint": "eslint --ignore --ignore-path .gitignore",
    "lint:js": "npm run --silent eslint -- .",
    "lint:changelog": "commitlint --from origin/master --to HEAD",
    "precommit": "lint-staged",
    "semantic-release": "semantic-release",
    "start": "nodemon --ignore src/public --watch src --ext js src/server.js",
    "test": "echo 'No tests specified' && exit 1"
  },
  "engines": {
    "node": ">=8"
  },
  "keywords": [],
  "author": "Ian Remmel <1182361+ianwremmel@users.noreply.github.com>",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "condition-circle": "^2.0.1",
    "ejs": "^2.6.1",
    "errorhandler": "^1.5.0",
    "eslint": "^5.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-compat": "^2.4.0",
    "eslint-plugin-eslint-comments": "^3.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-vue": "^4.5.0",
    "express": "^4.16.3",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "markdown-toc": "^1.2.0",
    "morgan": "^1.9.0",
    "nodemon": "^1.17.5",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.7",
    "rimraf": "^2.6.2",
    "semantic-release": "^15.7.1",
    "webpack": "^4.16.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "lint-staged": {
    "*.js": "npm run eslint -- "
  },
  "release": {
    "verifyConditions": "condition-circle"
  }
}
