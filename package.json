{
  "name": "@urbandoor/press",
  "version": "0.0.0-development",
  "description": "Progressive enhancement with VueJS",
  "bin": {},
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "jsdelivr": "./press.js",
  "style": "./press.css",
  "source": "./src/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/urbandoor/press.git"
  },
  "scripts": {
    "prebuild": "rimraf dist press.js press.js.map press.min.js press.min.js.map",
    "build": "npm-run-all build:*",
    "compile": "babel -d dist/${BUILD_TARGET} src --source-maps && ./compile.js",
    "build:cdn": "BUILD_TARGET=cdn webpack && BUILD_TARGET=cdn NODE_ENV=production webpack",
    "build:cjs": "BUILD_TARGET=cjs npm run compile && cp ./src/style.css ./dist/cjs/style.css",
    "build:esm": "BUILD_TARGET=esm npm run compile && cp ./src/style.css ./dist/esm/style.css",
    "build:readme": "markdown-toc -i --bullets='-' --maxdepth=3  README.md  && prettier --write README.md",
    "build:docs": "vue-styleguidist build",
    "clean": "rimraf dist styleguide press.js press.js.map press.min.js press.min.js.map press.css press.css.map press.min.css press.min.css.map",
    "eslint": "eslint --ignore --ignore-path .gitignore --ext js --ext vue",
    "lint": "npm-run-all lint:*",
    "lint:changelog": "commitlint --from origin/master --to HEAD",
    "lint:js": "npm run --silent eslint -- .",
    "lint:types": "npm-run-all lint:types:*",
    "lint:types:src": "tsc",
    "lint:types:features": "tsc --project ./tsconfig.features.json",
    "now-build": "echo 'now-build is a noop since the server just runs webpack dev server'",
    "plop": "plop",
    "semantic-release": "semantic-release",
    "start": "PORT=4000 AUTORUN=true nodemon --watch features --watch webpack.config.js --ext js features/server/index.js",
    "start:docs": "vue-styleguidist server",
    "test": "PORT=4000 AUTORUN=true DEBUG=press:test* wdio"
  },
  "engines": {
    "node": ">=8"
  },
  "keywords": [],
  "author": "Ian Remmel <1182361+ianwremmel@users.noreply.github.com>",
  "license": "MIT",
  "peerDependencies": {
    "autoprefixer": "^9.0.2",
    "cssnano": "^4.0.5",
    "daterangepicker": "^3.0.3",
    "jquery": "^3.3.1",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^3.0.0",
    "vue": "^2.5.16",
    "vue-loader": "^15.2.6",
    "vue-template-compiler": "^2.5.16"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@commitlint/cli": "^7.2.0",
    "@commitlint/config-conventional": "^7.0.1",
    "@semantic-release/changelog": "^3.0.0",
    "@semantic-release/exec": "^3.1.3",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.6",
    "@types/cucumber": "^4.0.4",
    "@types/daterangepicker": "^3.0.0",
    "@types/errorhandler": "0.0.32",
    "@types/express": "^4.16.0",
    "@types/jquery": "^3.3.11",
    "@types/lodash": "^4.14.115",
    "@types/lodash-webpack-plugin": "^0.11.3",
    "@types/mini-css-extract-plugin": "^0.2.0",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.11.4",
    "@types/uuid": "^3.4.3",
    "@types/webdriverio": "^4.10.4",
    "@types/webpack": "^4.4.14",
    "@types/webpack-dev-middleware": "^2.0.2",
    "@urbandoor/eslint-plugin-urbandoor": "^1.0.2",
    "autoprefixer": "^9.0.2",
    "babel-loader": "^8.0.4",
    "babel-plugin-lodash": "^3.3.4",
    "babel-preset-minify": "^0.5.0",
    "chai": "^4.2.0",
    "condition-circle": "^2.0.1",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.4",
    "daterangepicker": "^3.0.3",
    "ejs": "^2.6.1",
    "errorhandler": "^1.5.0",
    "eslint": "^5.6.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-compat": "^2.6.2",
    "eslint-plugin-eslint-comments": "^3.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jquery": "^1.3.2",
    "eslint-plugin-markdown": "^1.0.0-beta.8",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-vue": "^4.7.1",
    "eslint-plugin-webdriverio": "^1.0.1",
    "express": "^4.16.3",
    "glob": "^7.1.3",
    "husky": "^1.1.1",
    "jquery": "^3.3.1",
    "lint-staged": "^7.3.0",
    "lodash-webpack-plugin": "^0.11.5",
    "markdown-toc": "^1.2.0",
    "mini-css-extract-plugin": "^0.4.3",
    "mkdirp": "^0.5.1",
    "moment": "^2.22.2",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.3",
    "npm-run-all": "^4.1.3",
    "plop": "^2.1.0",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2",
    "semantic-release": "^15.9.17",
    "stoppable": "^1.0.6",
    "typescript": "^3.1.1",
    "vue": "^2.5.16",
    "vue-loader": "^15.4.2",
    "vue-styleguidist": "^1.8.5",
    "vue-template-compiler": "^2.5.16",
    "wdio": "^2.0.2",
    "wdio-cucumber-framework": "^2.2.2",
    "wdio-dot-reporter": "0.0.10",
    "wdio-junit-reporter": "^0.4.4",
    "wdio-sauce-service": "^0.4.11",
    "wdio-screenshots-cleanup-service": "0.0.7",
    "wdio-selenium-standalone-service": "0.0.10",
    "wdio-spec-reporter": "^0.1.5",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-dev-server": "^3.1.9"
  },
  "lint-staged": {
    "*.js": "npm run eslint -- ",
    "*.vue": "npm run eslint -- "
  },
  "release": {
    "verifyConditions": "condition-circle"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.1.2",
    "uuid": "^3.3.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e",
      "pre-commit": "lint-staged"
    }
  }
}
