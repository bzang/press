{
  "name": "@ianwremmel/press",
  "version": "0.0.0-development",
  "description": "Progressive enhancement with VueJS",
  "bin": {},
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "jsdelivr": "./press.js",
  "style": "./style.css",
  "source": "./src/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/ianwremmel/press.git"
  },
  "scripts": {
    "prebuild": "rimraf dist press.js press.js.map press.min.js press.min.js.map",
    "build": "npm-run-all build:*",
    "build:cdn": "BUILD_TARGET=cdn webpack && BUILD_TARGET=cdn NODE_ENV=production webpack",
    "build:cjs": "BUILD_TARGET=cjs babel -d dist/cjs src",
    "build:esm": "BUILD_TARGET=esm babel -d dist/esm src",
    "build:readme": "markdown-toc -i --bullets='-' --maxdepth=3  README.md  && prettier --write README.md",
    "commitmsg": "commitlint -e",
    "lint": "npm-run-all lint:*",
    "eslint": "eslint --ignore --ignore-path .gitignore",
    "lint:js": "npm run --silent eslint -- .",
    "lint:changelog": "commitlint --from origin/master --to HEAD",
    "precommit": "lint-staged",
    "semantic-release": "semantic-release",
    "test": "echo 'No tests specified' && exit 1"
  },
  "engines": {
    "node": ">=8"
  },
  "keywords": [],
  "author": "Ian Remmel <1182361+ianwremmel@users.noreply.github.com>",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^2.5.16"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "autoprefixer": "^9.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-minify": "^0.4.3",
    "chai": "^4.1.2",
    "condition-circle": "^2.0.1",
    "css-loader": "^1.0.0",
    "cssnano": "^4.0.3",
    "daterangepicker": "^3.0.3",
    "ejs": "^2.6.1",
    "errorhandler": "^1.5.0",
    "eslint": "^5.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-compat": "^2.4.0",
    "eslint-plugin-eslint-comments": "^3.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-vue": "^4.5.0",
    "express": "^4.16.3",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "lodash-webpack-plugin": "^0.11.5",
    "markdown-toc": "^1.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "mkdirp": "^0.5.1",
    "morgan": "^1.9.0",
    "nodemon": "^1.17.5",
    "npm-run-all": "^4.1.3",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.6",
    "prettier": "^1.13.7",
    "rimraf": "^2.6.2",
    "semantic-release": "^15.7.1",
    "stoppable": "^1.0.6",
    "vue": "^2.5.16",
    "vue-loader": "^15.2.6",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.16.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.4"
  },
  "lint-staged": {
    "*.js": "npm run eslint -- ",
    "*.vue": "npm run eslint -- "
  },
  "release": {
    "verifyConditions": "condition-circle"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0"
  }
}
